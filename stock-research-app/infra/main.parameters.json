{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "openAiDeploymentName": {
      "value": "${AZURE_OPENAI_DEPLOYMENT=gpt-4o-mini}"
    },
    "openAiModelName": {
      "value": "${AZURE_OPENAI_MODEL_NAME=gpt-4o-mini}"
    },
    "openAiModelVersion": {
      "value": "${AZURE_OPENAI_MODEL_VERSION=2024-07-18}"
    },
    "emailSenderAddress": {
      "value": "${EMAIL_SENDER_ADDRESS=}"
    },
    "emailDomain": {
      "value": "${EMAIL_DOMAIN=}"
    }
  }
}
